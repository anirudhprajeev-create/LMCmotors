rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /gallery/{document=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /vehicles/{document=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    // Add more collections here as needed
  }
}